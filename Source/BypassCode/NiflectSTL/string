#pragma once
#include <xmemory>

namespace std
{
template<class _Elem,
	class _Traits,
	class _Alloc>
	class basic_string
	{
	};

template<class _Elem>
	struct char_traits
	{
	};

template<> struct char_traits<char>
	{	// properties of a string or stream char element
	};

typedef basic_string<char, char_traits<char>, allocator<char> >
	string;
}