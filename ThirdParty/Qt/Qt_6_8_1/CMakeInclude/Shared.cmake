
add_library(Qt_6_8_1 SHARED IMPORTED)

if(UNIX)
	message(STATUS "Target Is on UNIX")
	set(OsType Linux)
	set(DlPost .so)
	set(SlPost .a)
elseif(APPLE)
	message(STATUS "Target Is on APPLE")
	message(FATAL_ERROR "OsType & DynamicLibraryPrefix & DynamicLibraryPostfix are not specified")
elseif(WIN32)
	message(STATUS "Target Is on WIN32")
	set(OsType Windows)
	set(DlPost .dll)
	set(SlPost .lib)
endif()
if("${CMAKE_SIZEOF_VOID_P}" EQUAL "8")
	set(ArchType 64)
else()
	set(ArchType 32)
endif()

set(QtRootPath "${RootThirdPartyPath}/Qt/Qt_6_8_1/build")
set(LibPathDebug "${QtRootPath}/${OsType}/${ArchType}/Debug/bin")
set(LibPathRelease "${QtRootPath}/${OsType}/${ArchType}/Release/bin")

include(${RootThirdPartyPath}/Qt/Qt_6_8_1/CMakeInclude/Deps.cmake)

set(QtIncRootPath ${QtRootPath}/include)
target_include_directories(${ModuleName} PRIVATE "${QtIncRootPath}")
set(incPathList)
foreach(dirName ${MyIncDirNames})
    list(APPEND incPathList "${QtIncRootPath}/${dirName}")
endforeach()
target_include_directories(${ModuleName} PRIVATE "${incPathList}")

set(binListDebug)
set(binListRelease)
if(WIN32)
	foreach(libName ${MyLibNames})
		list(APPEND binListDebug "${LibPathDebug}/${libPre}${libName}d${SlPost}")
		list(APPEND binListRelease "${LibPathRelease}/${libPre}${libName}${SlPost}")
	endforeach()
else()
	foreach(libName ${MyLibNames})
		list(APPEND binListDebug "${LibPathDebug}/${libPre}${libName}d${DlPost}")
		list(APPEND binListRelease "${LibPathRelease}/${libPre}${libName}${DlPost}")
	endforeach()
endif()

target_link_libraries(${ModuleName} PRIVATE
	$<$<CONFIG:Debug>:${binListDebug}>
	$<$<CONFIG:Release>:${binListRelease}>
)